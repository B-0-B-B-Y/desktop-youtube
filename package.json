{
  "name": "desktop-youtube",
  "version": "1.0.4",
  "author": "Bob Naydenov <bob.naydenov@hotmail.com>",
  "description": "An elegant desktop version of YouTube",
  "license": "GPL-3.0",
  "keywords": [
    "desktop",
    "app",
    "youtube",
    "videos",
    "games",
    "entertainment",
    "live",
    "music",
    "livestream",
    "comments",
    "share"
  ],
  "homepage": "./",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/B-0-B-B-Y/desktop-youtube.git"
  },
  "build": {
    "appId": "desktop.youtube",
    "productName": "YouTube Desktop",
    "mac": {
      "category": "public.app-category.entertainment",
      "icon": "app/build/icon.icns"
    },
    "win": {
      "target": "NSIS",
      "icon": "app/build/icon.ico"
    },
    "linux": {
      "category": "Network",
      "icon": "app/build/"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "package": "yarn build",
    "postpackage": "electron-packager ./ --out=./builds",
    "dist-win": "electron-builder --win"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^3.5.3",
    "electron-builder": "^22.7.0",
    "eslint": "^7.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.15.1",
    "electron": "^15.5.5",
    "electron-updater": "^2.21.4",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
